<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties resource="db.properties" />
	<typeAliases>
		<typeAlias type="com.team5.funthing.user.model.vo.MemberVO" alias="member"/>
		<typeAlias type="com.team5.funthing.user.model.vo.NoticeVO" alias="notice"/>
		<typeAlias type="com.team5.funthing.user.model.vo.ProjectVO" alias="project"/>
		<typeAlias type="com.team5.funthing.user.model.vo.BoardVO" alias="board"/>

		<typeAlias type="com.team5.funthing.user.model.vo.ProjectBoardVO" alias="projectboard"/>

		<typeAlias type="com.team5.funthing.user.model.vo.CSBoardVO" alias="csboard"/>
		<typeAlias type="com.team5.funthing.user.model.vo.DeliveryAddressVO" alias="deliveryAddress"/>
		<typeAlias type="com.team5.funthing.user.model.vo.ReportVO" alias="report"/>
		<!-- user : admin -->
		<typeAlias type="com.team5.funthing.admin.model.vo.AdminCSBoardVO" alias="adminCSBoard"/>
		<typeAlias type="com.team5.funthing.admin.model.vo.AdminNoticeBoardVO" alias="adminNoticeBoard"/>

	</typeAliases>
	<mappers>
		<mapper resource="mappers/member-mapper.xml" />
		<mapper resource="mappers/notice-mapper.xml"/>
		<mapper resource="mappers/project-mapper.xml" />
		<mapper resource="mappers/board-mapper.xml" />

		<mapper resource="mappers/projectboard-mapper.xml" />

		<mapper resource="mappers/csboard-mapper.xml" />
		<mapper resource="mappers/deliveryAddress-mapper.xml"/>
		<mapper resource="mappers/report-mapper.xml" />
		<!-- user : admin -->
		<mapper resource="mappers/adminCSBoard-mapper.xml" />
		<mapper resource="mappers/adminNoticeBoard-mapper.xml"/>

	</mappers>
</configuration>
