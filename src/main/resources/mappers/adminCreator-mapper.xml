<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="AdminCreatorDAO">

	<select id="getCreatorList" parameterType="adminCreator" resultType="adminCreator">
		SELECT * FROM makerMember
	</select>
	
	<select id="getCreator" parameterType="adminCreator" resultType="adminCreator">
		SELECT * FROM makerMember WHERE creator = #{creator}
	</select>
	
	<delete id="deleteCreator" parameterType="adminCreator">
		DELETE FROM makerMember WHERE creator = #{creator}
	</delete>

</mapper>
